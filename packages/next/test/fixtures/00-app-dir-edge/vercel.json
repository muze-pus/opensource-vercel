{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "probes": [
    {
      "path": "/edge",
      "status": 200,
      "mustContain": "edge"
    },
    {
      "path": "/edge",
      "status": 200,
      "headers": {
        "RSC": "1"
      },
      "mustContain": ":{",
      "mustNotContain": "<html"
    },
    {
      "path": "/edge",
      "status": 200,
      "headers": {
        "RSC": "1"
      },
      "responseHeaders": {
        "content-type": "application/octet-stream"
      }
    },
    {
      "path": "/",
      "status": 200,
      "mustContain": "page"
    },
    {
      "path": "/",
      "status": 200,
      "headers": {
        "RSC": "1"
      },
      "mustContain": ":{",
      "mustNotContain": "<html"
    },
    {
      "path": "/",
      "status": 200,
      "headers": {
        "RSC": "1"
      },
      "responseHeaders": {
        "content-type": "application/octet-stream"
      }
    }
  ]
}
